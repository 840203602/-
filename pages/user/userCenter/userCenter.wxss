
.container {
  background-color: white;
  padding-top: 0.001px;
}

.userInfo {
  background: #c34b58;
  display: flex;
  flex-direction: column; /**从上到下布局**/
  align-items: center; /**定义项目在交叉轴上如何对齐。
**/
  width: 100%;
}

.userAvatar {
  margin-top: 40rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  vertical-align: middle;
}

.userName {
  color: white;
  font-size: 14rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
}

.userInfo_list {
  width: 100%;
}

.itemCell {
  padding: 15rpx;
  height: 45rpx;
  border-bottom: 1px solid #dadada;
  position: relative;
  display: flex;
  align-items: center;
}

.itemCellImage {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.itemCellImageValue {
  width: 30rpx;
  height: 30rpx;
}

.itemCellText {
  display: inline-block;
}

.itemCellTextValue {
  font-size: 14rpx;
  color: #939393;
}

.jinruImage {
  position: absolute;
  right: 20rpx;
  width: 15rpx;
  height: 15rpx;

}
