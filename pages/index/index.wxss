/**index.wxss**/

.backgroundView {
  background-color: white;
  width: 100%;
}

.slide-image {
  width: 100%;
  height: 300rpx;
}

.addressView {
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  background-color: #efefef;
}

.address {
  font-size: 30rpx;
  color: black;
  padding-left: 15rpx;
}

.changeAddress {
  font-size: 30rpx;
  color: c34b58;
  padding-right: 50rpx;
  position: relative;
}

.arrow-right::after {
  display: block;
  position: absolute;
  top: 25rpx;
  right: 15rpx;
  width: 30rpx;
  height: 30rpx;
  content: ' ';
  background-image: url(data:image/png;base64,);
  background-size: 30rpx 40rpx;
  background-repeat: no-repeat;
  background-position: 95% center;
  background-position: -webkit-calc(100%) center;
  background-position: calc(100%) center;
}

.jinruImage {
  width: 15rpx;
  height: 15rpx;
  position: absolute;
  right: 15rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
}

.clear {
  clear: both;
}

.teaList {
  margin-top: 15rpx;
  flex-direction: row;
  flex-wrap: wrap;
  background-color: white;
}

.teaView {
  width: 230rpx;
  float: left;
  border-radius: 15rpx;
  margin-left: 15rpx;
  flex-direction: column;
  background-color: #e6e6e6;
  margin-bottom: 15rpx;
}

.teaImage {
  width: 230rpx;
  height: 270rpx;
  background-color: #e6e6e6;
  border-radius: 10rpx 10rpx 0 0;
}

.teaName {
  font-size: 28rpx;
  color: black;
  margin-top: 15rpx;
  margin-left: 5rpx;
  text-align: center;
}

.teaType {
  font-size: 20rpx;
  color: #c34b58;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  margin-left: 5rpx;
  text-align: center;
}
